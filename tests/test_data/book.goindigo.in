<script type="text/javascript">
    $(document).ready(function () {
        $("#FlightSearchLoginPop").click(function () {
            $("#availabilityForm").submit();
        });
        $(".ContinueBookingBtn").click(function () {
            $("#availabilityForm").submit();
        });

        $('.loginPopup').focus(function () {
            $(this).removeClass('error').parent().find('.hint').remove();
        });

        $(document).on('keypress', '.loginPopup', function (e) {
            if (e.keyCode == 13) {
                $('#MemberLoginBtn').trigger('click');
                e.preventDefault();
                return false;
            }
        });


        function validatepop() {
            var value = $("#memberId").val();
            var regex_cell = /[^[0-9]]*/gi;
            var new_value = value.replace(regex_cell, '');
            $("#memberId").val(new_value);
            return true;
        }
        function FlightSearchLoginValidation(memberId, memberPassword) {
            var flag = true;

            var countryCode = $("#txtloginMemberCountryCode");
            var trimmedcountryCode = countryCode.val().replace(/\b0+/g, "");
            if (countryCode.val().length == 0) {
                $(countryCode).addClass('error').after('<span class="hint "><div class="hint-message"><div class="hint-arrow"></div>Enter country code.</div></span>');
                flag = false;
            } else if (trimmedcountryCode.length == 0) {
                $(countryCode).addClass('error').after('<span class="hint "><div class="hint-message"><div class="hint-arrow"></div>Invalid country code.</div></span>');
                flag = false;
            }
            countryCode.val(countryCode.val().replace(/^0+/, ''));
            var errorTextUid = '';
            if ($('#logViaUserID_ppopup').is(':checked')) {
                errorTextUid = 'User ID is required.';
            }
            else {
                errorTextUid = 'Mobile Number is required.';
            }

            if (memberId.val() == "" && memberPassword.val() == "") {
                memberId.addClass('error').after('<span class="hint" id="indiGoAvailability_fareRules_DoesAgreeToTerms_hint"><div class="hint-message"><div class="hint-arrow"></div>' + errorTextUid + '</div></span>');
                memberPassword.addClass('error').after('<span class="hint" id="indiGoAvailability_fareRules_DoesAgreeToTerms_hint"><div class="hint-message"><div class="hint-arrow"></div>Password is required.</div></span>');
                flag = false;
            }
            else if (memberId.val() == "") {
                memberId.addClass('error').after('<span class="hint" id="indiGoAvailability_fareRules_DoesAgreeToTerms_hint"><div class="hint-message"><div class="hint-arrow"></div>' + errorTextUid +'</div></span>');
                flag = false;
            }
            else if (memberId.attr('id') == 'viaMobileNomemberId' && !$.isNumeric(memberId.val()))
            {
                memberId.addClass('error').after('<span class="hint" id="indiGoAvailability_fareRules_DoesAgreeToTerms_hint"><div class="hint-message"><div class="hint-arrow"></div>Please Enter Valid Mobile Number.</div></span>');
                flag = false;
            }
            else if (memberPassword.val() == "") {
                memberPassword.addClass('error').after('<span class="hint" id="indiGoAvailability_fareRules_DoesAgreeToTerms_hint"><div class="hint-message"><div class="hint-arrow"></div>Required.</div></span>');
                flag = false;
            }
            memberId.click(function () {
                if (memberId.next().hasClass("hint")) {
                    memberId.next().remove();
                }
            });
            memberPassword.click(function () {
                if (memberPassword.next().hasClass("hint")) {
                    memberPassword.next().remove();
                }
            });
            return flag;
        }

        $("#MemberLoginBtn").click(function () {

            var memberId;
            var memberPassword;
            if ($('#logViaUserID_ppopup').is(':checked')) {
                memberId = 'memberId';
                memberPassword = 'memberPassword';
                $('#hdnMemberUserId').val($('#memberId').val());
            } else {
                memberId = 'viaMobileNomemberId';
                memberPassword = 'viaMobileNomemberPassword';
            }

            var memberIdvalue = $('#' + memberId).val();
            var memberPasswordvalue = $('#' + memberPassword).val();

            if (FlightSearchLoginValidation($('#' + memberId), $('#' + memberPassword))) {

                if (!$('#logViaUserID_ppopup').is(':checked')) {
                    if (!loginValidation('txtloginMemberCountryCode', memberId, 'hdnMemberUserId','none')) {
                        return false;
                    }
                }

                if (memberIdvalue != "" && memberPasswordvalue != "") {
                    $("#ajaxLoader").show();
                    $("#data").hide();
                    $.ajax({
                        url: '/Member/LoginAEM',
                        type: "POST",
                        data: {
                            'memberLogin.Username': $('#hdnMemberUserId').val(),
                            'memberLogin.Password': memberPasswordvalue
                        },
                        success: function (data) {

                        if (data.indiGoError.errors.length > 0) {
                                if ($('#logViaUserID_ppopup').is(':checked')) {
                                    $('#data').html('The system did not recognize the user login information and/or password that you entered. Please check and try again.<br\> You can try logging in using your Mobile Number or Continue without login.').show();
                                } else {
                                    $('#data').html('The system did not recognize the user login information and/or password that you entered. Please check and try again.<br\> You can try logging in using your User ID or Continue without login.').show();
                                }

                                $("#ajaxLoader").hide();
                            }
                            else {
                                try {
                                    SetAEMStatusCookie('Member');
                                }
                                catch (e) { }
                                $("#availabilityForm").submit();
                            }
                        }
                    });
                }
            }
        });

        $("#AgentLoginBtn").click(function () {          
            var agentId = $("#AgentId");
            var agentPassword = $("#AgentPassword");
            FlightSearchLoginValidation(agentId, agentPassword);
            if (agentId.val() != "" && agentPassword.val() != "") {
                $("#ajaxLoaderAgent").show();
                $("#dataAgent").hide();;
                $.post('/Agent/AgentLogin',
                    {
                        'agentLogin.Username': agentId.val(),
                        'agentLogin.Password': agentPassword.val()
                    },
                    function (data) {
                        if (data.indexOf("The system did not recognize the user Log In information") != -1) {
                            $("#dataAgent").html("The system did not recognize the user Log In information and/or password that you entered. Please check and try again.").show();
                            $("#ajaxLoaderAgent").hide();
                        }
                        else {
                            $("#availabilityForm").submit();                          
                        }                                         
                    });
            }
        });
    });
</script>







