    //$(document).ready(function() {
    //    $('#linkTravelInformation').hover(function () { $('#travelInformationPopUp').modal('show'); $(this).focus(); }, function () {});
    //});
    $(document).ready(function () {
        if ($(window).width() > 767) {
            $(".travel_infocontent").fadeOut();
            var currentMousePos = { x: -1, y: -1 };

            $('#linkTravelInformation').focus(function () {
                currentMousePos = { x: -1, y: -1 };
            });

            $(".travel_infopopup").focus(function () {
                $(this).addClass('active');
                $(this).find('a').focus(0);
                //$(".travel_infocontent").show();
                $(".travel_infocontent").hide();
                $(".overlay2").show();
            });
            $('.nav-pills.pull-right > li:not(.travelInfoholder) a').focus(function () {
                $(".travel_infocontent").hide();
                $(".travel_infopopup").removeClass("active");
                $(".overlay2").hide();
            });
            $(".travel_infopopup").hover(function () {
                $(this).addClass('active');
                //$(".travel_infocontent").fadeIn();
                $(".travel_infocontent").hide();
                //$(".overlay2").fadeIn();
                $(".overlay2").hide();
            });
            $(".overlay2").mousemove(function (event) {
                if (currentMousePos.x == -1) {
                    currentMousePos.x = event.pageX;
                    currentMousePos.y = event.pageY;
