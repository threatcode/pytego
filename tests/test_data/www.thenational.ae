                        <script>

$(window).load(loadFunction);
$(window).resize(loadFunction);

function loadFunction(event) {
    setCarouselItemsWidth(calculateNumCarouselItems());
    setCarouselControlsHeight();
}

function setCarouselControlsHeight(){
    var carousel = $(".story-carousel");
    var controls = $( ".story-carousel .control");
    var controlHeight = controls.outerHeight();
    var carouselHeight = carousel.outerHeight();
    var setHeight = ((carouselHeight-controlHeight)/2)+'px';

    controls.each(function() {
      $(this).css('margin-top',setHeight);
      $(this).children('i').css('color', '#fff');
    });
}

function setCarouselItemsWidth(numCarouselItems){
    var viewable = $( ".story-carousel .viewable");
    var viewableWidth = viewable.innerWidth();
    var innerItems = $( ".story-carousel .viewable .inner-content .story-carousel-item");
    var setCarouselItemsWidth = (viewableWidth / numCarouselItems);

    innerItems.each(function() {
        let padding = parseInt($(this).css('padding-right'));
        let setWidth = (setCarouselItemsWidth - (padding * 2) )+'px';
        $(this).width(setWidth);
    });
}

function calculateNumCarouselItems(){
    var windowWidth = parseInt($(window).width());
    var numCarouselItems = 4;
    if (windowWidth < 1000) {
        numCarouselItems = 3;
    }
    if (windowWidth < 750) {
        numCarouselItems = 2;
    }
    if (windowWidth < 600) {
        numCarouselItems = 1;
    }
    return numCarouselItems;
}

</script>

