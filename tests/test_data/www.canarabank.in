			<script type="text/javascript">
	$('.leftpanel-btn').click(function(e){
		e.preventDefault();
		drawerAnimate();
	})
	drawerAnimate();
	function drawerAnimate() {
	    if($('.drawer-menu').hasClass("in")) { 
	    	console.log('show');      
	        $('.drawer-menu').animate({'right': '-5%'}, 400);
	        $('.drawer-menu').removeClass('in');
	        $('.leftpanel-btn').find('.span-show').css({
				'display': 'block',
			});	
			$('.leftpanel-btn').find('.span-hide').css({
				'display': 'none',
			});					      
	    }
	    else {
	    	console.log('hide');
	        $('.drawer-menu').animate({'right': '0'}, 400);
	        $('.drawer-menu').addClass('in');  
	        $('.leftpanel-btn').find('.span-show').css({
				'display': 'none',
			});	
			$('.leftpanel-btn').find('.span-hide').css({
				'display': 'block',
			});	 
	    }
	}
</script>
